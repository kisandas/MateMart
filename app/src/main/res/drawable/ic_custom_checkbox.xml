<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Checked and enabled state -->
<item android:drawable="@drawable/ic_checkbox_selected" android:state_checked="true" android:state_enabled="true" />

<!-- Unchecked and enabled state -->
<item android:drawable="@drawable/ic_checkbox_unselected" android:state_checked="false" android:state_enabled="true" />

<!-- Disabled state -->
<item android:drawable="@drawable/ic_checkbox_unselected_disable" android:state_enabled="false" />
</selector>